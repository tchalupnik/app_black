{
  "model": "Fujitsu Air Conditioner UTY-VMSX",
  "registers_base": [
    {
      "base": 53,
      "length": 11,
      "register_type": "input",
      "registers": [
        {
          "name": "Operating mode number",
          "address": 53,
          "write_address": 1,
          "entity_type": "writeable_sensor_discrete",
          "state_class": "measurement",
          "value_type": "U_WORD"
        },
        {
          "name": "ON OFF status number",
          "address": 54,
          "write_address": 2,
          "entity_type": "writeable_binary_sensor_discrete",
          "value_type": "U_WORD",
          "payload_on": "2",
          "payload_off": "1"
        },
        {
          "name": "Set temperature",
          "address": 55,
          "write_address": 3,
          "entity_type": "writeable_sensor",
          "state_class": "measurement",
          "value_type": "U_WORD",
          "filters": [
            {
              "multiply": 0.25
            }
          ],
          "write_filters": [
            {
              "encode_temperature": 4
            }
          ]
        },
        {
          "name": "Airflow status",
          "address": 56,
          "write_address": 4,
          "entity_type": "writeable_sensor_discrete",
          "state_class": "measurement",
          "value_type": "U_WORD"
        },
        {
          "name": "Indoor temperature",
          "address": 57,
          "entity_type": "sensor",
          "state_class": "measurement",
          "value_type": "U_WORD",
          "filters": [
            {
              "multiply": 0.25
            }
          ]
        },
        {
          "name": "Vertical air direction position status",
          "address": 59,
          "write_address": 5,
          "entity_type": "writeable_sensor_discrete",
          "state_class": "measurement",
          "value_type": "U_WORD"
        },
        {
          "name": "Horizontal air direction position status",
          "address": 60,
          "write_address": 6,
          "entity_type": "writeable_sensor_discrete",
          "state_class": "measurement",
          "value_type": "U_WORD"
        },
        {
          "name": "Filter sign",
          "address": 62,
          "entity_type": "text_sensor",
          "value_type": "U_WORD",
          "x_mapping": {
            "0": "Normal",
            "1": "Filter change"
          }
        }
      ]
    }
  ],
  "additional_sensors": [
    {
      "name": "Operating mode",
      "source": "operating_mode_number",
      "entity_type": "select",
      "x_mapping": {
        "1": "Auto",
        "2": "Cool",
        "3": "Dry",
        "4": "Heat",
        "5": "Fan"
      }
    },
    {
      "name": "ON OFF switch",
      "source": "on_off_status_number",
      "entity_type": "switch",
      "x_mapping": {
        "1": "OFF",
        "2": "ON"
      },
      "payload_off": "OFF",
      "payload_on": "ON"
    },
    {
      "name": "Airflow",
      "source": "airflow_status",
      "entity_type": "select",
      "x_mapping": {
        "1": "Auto",
        "2": "Quiet",
        "3": "Low",
        "4": "Med",
        "5": "High",
        "6": "Med-Low",
        "7": "Med-High"
      }
    },
    {
      "name": "Vertical air direction",
      "source": "vertical_air_direction_position_status",
      "entity_type": "select",
      "x_mapping": {
        "1": "Swing",
        "2": "Position 1",
        "3": "Position 2",
        "4": "Position 3",
        "5": "Position 4"
      }
    },
    {
      "name": "Horizontal air direction",
      "source": "horizontal_air_direction_position_status",
      "entity_type": "select",
      "x_mapping": {
        "1": "Swing",
        "2": "Position 1",
        "3": "Position 2",
        "4": "Position 3",
        "5": "Position 4",
        "6": "Position 5"
      }
    }
  ]
}