{
  "model": "Liquid sensor",
  "set_base": {
    "set_address_address": 0,
    "set_baudrate": {
      "address": 1,
      "possible_baudrates": {
        "1200": 0,
        "2400": 1,
        "4800": 2,
        "9600": 3,
        "19200": 4,
        "38400": 5,
        "57600": 6,
        "115200": 7
      }
    }
  },
  "registers_base": [
    {
      "base": 4,
      "length": 2,
      "register_type": "holding",
      "registers": [
        {
          "name": "Measurement Value",
          "address": 4,
          "unit_of_measurement": "cm",
          "state_class": "measurement",
          "device_class": "distance",
          "value_type": "S_WORD",
          "filters": [{ "multiply": 0.1 }]
        }
      ]
    }
  ],
  "additional_sensors": [
    {
      "name": "Measurement Volume",
      "source": "measurement_value",
      "unit_of_measurement": "l",
      "device_class": "volume_storage",
      "state_class": "measurement",
      "formula": "width * length * X * 10",
      "config_keys": ["width", "length"]
    }
  ]
}
